### Welcome to the InfluxDB Enterprise configuration file (generated by Ansible).

# Once every 24 hours InfluxDB Enterprise will report anonymous data to m.influxdb.com
# The data includes raft id (random 8 bytes), os, arch, version, and metadata.
# We don't track ip addresses of servers reporting. This is only used
# to track the number of instances running and the versions, which
# is very helpful for us.

# Change this option to true to disable reporting.
reporting-disabled = {{ influx_data_disable_reporting|lower }}
{% if influx_data_hostname is defined and influx_data_hostname != None %}
hostname = "{{ influx_data_hostname }}"
{% else %}
hostname = "{{ ansible_hostname }}"
{% endif %}
meta-tls-enabled = {{ influx_data_meta_tls_enabled|lower }}

[enterprise]
  registration-enabled = {{ influx_data_registration_enabled|lower }}
  registration-server-url = "{{ influx_data_registration_server_url|default("", true) }}"
  license-key = "{{ influx_enterprise_license_key|default("", true) }}"
  license-path = "{{ influx_data_license_path|default("", true) }}"

# The meta section can be disregarded
[meta]
  enabled = false
  dir = "/var/lib/influxdb/meta"

[data]
  enabled = {{ influx_data_enabled|lower }}
  dir = "{{ influx_data_dir }}"
  max-wal-size = {{ influx_data_max_wal_size }}
  wal-flush-interval = "{{ influx_data_wal_flush_interval }}"
  wal-partition-flush-delay = "{{ influx_data_wal_partition_flush_delay }}"
  wal-dir = "{{ influx_data_wal_dir }}"
  wal-logging-enabled = {{ influx_data_wal_logging_enabled|lower }}
  wal-ready-series-size = {{ influx_data_wal_ready_series_size }}
  wal-compaction-threshold = {{ influx_data_wal_compaction_threshold }}
  wal-max-series-size = {{ influx_data_wal_max_series_size }}
  wal-flush-cold-interval = "{{ influx_data_wal_flush_cold_interval }}"
  wal-partition-size-threshold = {{ influx_data_wal_partition_size_threshold }}
  query-log-enabled = {{ influx_data_query_log_enabled|lower }}
  cache-max-memory-size = {{ influx_data_cache_max_memory_size }}
  cache-snapshot-memory-size = {{ influx_data_cache_snapshot_memory_size }}
  cache-snapshot-write-cold-duration = "{{ influx_data_cache_snapshot_write_cold_duration }}"
  compact-full-write-cold-duration = "{{ influx_data_compact_full_write_cold_duration }}"
  max-points-per-block = {{ influx_data_max_points_per_block }}
  data-logging-enabled = {{ influx_data_data_logging_enabled|lower }}
  
[cluster]
  force-remote-mapping = {{ influx_data_force_remote_mapping|lower }}
  write-timeout = "{{ influx_data_write_timeout }}"
  shard-writer-timeout = "{{ influx_data_shard_writer_timeout }}"
  max-remote-write-connections = {{ influx_data_max_remote_write_connections }}
  shard-mapper-timeout = "{{ influx_data_shard_mapper_timeout }}"

[retention]
  enabled = {{ influx_data_retention_enabled|lower }}
  check-interval = "{{ influx_data_retention_check_interval }}"

[shard-precreation]
  enabled = {{ influx_data_shard_precreation_enabled|lower }}
  check-interval = "{{ influx_data_shard_precreation_check_interval }}"
  advance-period = "{{ influx_data_shard_precreation_advance_period }}"

[admin]
  enabled = {{ influx_data_admin_enabled|lower }}
  bind-address = "{{ influx_data_admin_bind_address }}"
  https-enabled = {{ influx_data_admin_https_enabled|lower }}
  https-certificate = "{{ influx_data_admin_https_certificate }}"

[monitor]
  store-enabled = {{ influx_data_monitor_store_enabled|lower }}
  store-database = "{{ influx_data_monitor_store_database }}"
  store-interval = "{{ influx_data_monitor_store_interval }}"

[subscriber]
  enabled = {{ influx_data_subscriber_enabled|lower }}
  
[http]
  enabled = {{ influx_data_http_enabled|lower }}
  bind-address = "{{ influx_data_http_bind_address_hostname|default("", true) }}:{{ influx_data_http_bind_address_port }}"
  auth-enabled = {{ influx_data_http_auth_enabled|lower }}
  log-enabled = {{ influx_data_http_log_enabled|lower }}
  write-tracing = {{ influx_data_http_write_tracing|lower }}
  pprof-enabled = {{ influx_data_http_pprof_enabled|lower }}
  https-enabled = {{ influx_data_http_https_enabled|lower }}
  https-certificate = "{{ influx_data_http_https_certificate }}"

[continuous_queries]
  log-enabled = {{ influx_data_cq_log_enabled|lower }}
  enabled = {{ influx_data_cq_enabled|lower }}
  run-interval = "{{ influx_data_cq_run_interval }}"

[hinted-handoff]
  enabled = {{ influx_data_hh_enabled|lower }}
  dir = "{{ influx_data_hh_dir }}"
  max-size = {{ influx_data_hh_max_size }}
  max-age = "{{ influx_data_hh_max_age }}"
  retry-rate-limit = {{ influx_data_hh_retry_rate_limit }}
  retry-interval = "{{ influx_data_hh_retry_interval }}"
  retry-max-interval = "{{ influx_data_hh_retry_max_interval }}"
  purge-interval = "{{ influx_data_hh_purge_interval }}"
